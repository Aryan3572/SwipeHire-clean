generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}


model User {
  id            String   @id @default(uuid())
  name          String
  email         String   @unique
  password      String
  resumeUrl     String?
  skills        String[]
  rolePreference String?
  lookingFor    String?  // "job" or "internship"
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
